{
   "app": {
      "launch": {
         "local_path": "index.html"
      }
   },
   "description": "A simple app that store tasks, notes, and just about anything locally on your device! No internet access required!",
   "icons": {
      "128": "128.png",
      "16": "16.png"
   },
   "manifest_version": 2,
   "name": "todoo",
   "offline_enabled": true,
   "options_page": "options/options.html",
   "permissions": [ "tabs", "bookmarks", "http://www.google-analytics.com/ga.js", "http://*.google.com/", "unlimitedStorage" ],
   "version": "1.9.0.1",
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
